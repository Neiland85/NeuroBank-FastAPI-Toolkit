[vulture]
paths = app/
exclude = 
    **/tests/**
    **/__pycache__/**
    **/migrations/**
    **/alembic/versions/**
    .venv/**
    venv/**
min_confidence = 60
ignore_names = 
    *test*
    *mock*
    *fixture*
    __*__
ignore_decorators = 
    @app.get
    @app.post
    @app.put
    @app.delete
    @app.patch
    @router.get
    @router.post
    @router.put
    @router.delete
    @router.patch
    @pytest.fixture
    @lru_cache
verbose = true

