{
  "version": 2,
  "functions": {
    "api/index.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  },
  "regions": [
    "iad1"
  ],
  "buildCommand": null,
  "devCommand": null,
  "installCommand": "pip install -r api/requirements.txt"
}